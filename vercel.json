{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "node_modules/.prisma/**",
        "prisma/**"
      ]
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}